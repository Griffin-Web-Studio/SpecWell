{
    "[sass]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "syler.sass-indented"
    },
    "[jsx-tags]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.language": "en-GB",
    "cSpell.flagWords": [
        "teh->the",
        "adn->and",
        "nad->and",
        "fo->of",
        "ot->to"
    ],
    "cSpell.words": [
        "Flickity",
        "Gtheme",
        "bloginfo",
        "customizer",
        "initialise",
        "initialiser",
        "postid",
        "simultaniusly",
        "smallscreen",
        "woocommerce"
    ],
    "editor.fontLigatures": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
}
